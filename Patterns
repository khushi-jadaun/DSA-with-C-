#include<iostream>
using namespace std;
void pattern7(int n){
    for(int i=0;i<n;i++){
        for(int j=1;j<(n-i);j++){
            cout<<" ";
        }
        
            for(int j=0;j<=(i);j++){
                cout<<"*";
            }
            for(int j=0;j<(i);j++){
                cout<<"*";
            }
            cout<<endl;
        
    }
}
int main(){
    int n;
    cin>>n;
    pattern7(n);
}

