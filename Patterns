#include <iostream>
using namespace std;
void pattern(int n){
    for(int i=0;i<=n;i++){
        for(int j=1;j<=i;j++){
            cout<<i <<" ";
        }
        cout<<" "<<endl;
    }}
int main(){
        int n;
        cin>>n;
        pattern(n);
    
  
    return 0;
}

