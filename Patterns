#include<iostream>
using namespace std;
void pattern9(int n){
    for(int i=0;i<n;i++){
        for(int j=0;j<=i;j++){
            cout<<"*";
        }
       cout<<endl;
    }
}
void pattern10(int n){
    for(int i=0;i<n;i++){
        for(int j=1;j<n-i;j++){
            cout<<"*";
        }
       cout<<endl;
    }
}

int main(){
    int n;
    cin>>n;
    pattern9(n);
    pattern10(n);
}

